

<div class="card-group" id="first">
	<div class="card">
		<img src="..." class="card-img-top" alt="...">
		<div class="card-body">
			<h5 class="card-title">Card title</h5>
			<p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
			<p class="card-text"><small class="text-body-secondary">Last updated 3 mins ago</small></p>
		</div>
	</div>
</div>
@section Scripts {
	<script src="~/js/travel.js"></script>
	<script>
		console.log(spots.CommonFormat.Section);

		for (let i = 0; i < 2; i++) {
			var newDiv = document.createElement("div");
			newDiv.setAttribute("class", "card");
			newDiv.setAttribute("id", "imgs");
			document.getElementById("first").appendChild(newDiv);
			var newimg = document.createElement("img");			
			newimg.src = spots.CommonFormat.Section[i].file.img[i].text;
			newimg.setAttribute("id", "photo");
			newimg.setAttribute("class", "card-img-top");
			newimg.setAttribute("alt", "...");
			document.getElementById("imgs").appendChild(newimg);



			var newcontext = document.createElement("p");
			console.log(spots.CommonFormat.Section[i].xbody);
			newcontext.textContent = spots.CommonFormat.Section[i].xbody;
			document.getElementById("imgs").appendChild(newcontext);
		}
	</script>
}